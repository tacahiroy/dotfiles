if alias vim >/dev/null 2>&1; then
	unalias vim
fi

alias ls='ls -F --color=auto'
alias ll='ls -l'
alias la='ls -a'
alias lla='ls -la'
alias ltr='ls -ltr'
alias llh='ls -lh'
alias sl='ls'
alias gi='git'
alias vi='vim'
alias df='df -h'
alias g='git'
alias gi='git'
alias gu='git up'
alias gl='git log'
alias lines='grep -c ""'
alias b='cd -'
alias cda='cd -P .'
alias wo='workon'
